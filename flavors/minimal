set repo file:///repo
set curl [curl]
set available [packlim::available $curl $repo]

foreach package {skeleton toybox sdaemons loksh packlim mandoc initramfs linux-libre isolinux rfkill dhcpcd iw wpa_supplicant iptables}  {
	packlim::install $curl $repo {*}$available $package locked ""
}

packlim::purge
