#!/bin/sh

cd /etc/init.d
for i in *
do
	[ -x "$i" ] && "./$i" start
done

echo "
Welcome to RLSD!

This is free software written by many authors across the globe and you may
redistribute it, under certain conditions and limitations; see /usr/share/doc
for details.

This software comes with ABSOLUTELY NO WARRANTY; use at your own risk.
"

exec fakelogin
